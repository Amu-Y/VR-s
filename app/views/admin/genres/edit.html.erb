<header class="contents__header">
  <%= render 'layouts/header' %>
</header>
<article class="rooms">
  <section class="rooms__text">
    <h1 class="rooms__text_title">
      Genre edit
      </h1>
      <%= form_with model: [:admin, @genre], local:true do |f| %>
        <%= f.text_field :name, placeholder: "ジャンル名", size: "30", class: "form-control mr-3" %>
        <%= f.submit '　変更を保存　', class: "btn btn-sm btn-secondary" %>
      <% end %>
  </section>
</article>

